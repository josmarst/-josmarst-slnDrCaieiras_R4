@model IEnumerable<atribuicaoAulas.BloqueioCadastro>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.bloqueado)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.databloqueio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CPFBloqueio)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.bloqueado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.databloqueio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CPFBloqueio)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.idBloqueioCadastro }) |
            @Html.ActionLink("Details", "Details", new { id=item.idBloqueioCadastro }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.idBloqueioCadastro })
        </td>
    </tr>
}

</table>
